
@model CarDetailsViewModel





<div class="card col-6 col-lg-3  game-card">
    <img src="@Model.ImageUrl"
         class="card-img-top img-fluid card-image rounded" data-toggle="tooltip"
         data-placement="bottom" title="@Model.Make @Model.Year">
    <div class="card-body">
        <ul class="list-group list-group-flush">
            <li class="list-group-item text-center">Plate # @Model.PlateNumber</li>
        </ul>

    </div>
    <div>
    <a href="/Cars/Edit?carId=@Model.Id" class="btn btn-block btn-danger">Edit car</a>
    </div>
     <div>
     <a href="/Cars/All" class="btn btn-block btn-info">All car</a>
     </div>
</div>